    <releaseNotes>https://github.com/mbientlab/MetaWear-SDK-CSharp/releases/tag/1.0.0</releaseNotes>
    <tags>bluetoothle metawear metamotion metatracker sensors netstandard2.0 net4.6.1 uap</tags>
    <dependencies>
      <dependency id="MetaWear" version="[1.0, 2.0)" />

      <group targetFramework="uap10.0">
        <dependency id="Microsoft.NETCore.UniversalWindowsPlatform" version="6.0.8" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="build\**" target="build" />
    <file src="..\MetaWear.Win10.Uwp\bin\Release\MbientLab.MetaWear.Uwp.dll" target="lib\uap10.0" />
    <file src="..\MetaWear.Win10.Uwp\bin\Release\MbientLab.MetaWear.Uwp.xml" target="lib\uap10.0" />
    <file src="..\MetaWear.Win10.DotNet\bin\Release\MbientLab.MetaWear.DotNet.dll" target="lib\net461" />
